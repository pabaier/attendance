<div class="jumbotron">
    <h1>Courses</h1>
    <select class="form-select my-2" id="year-select" aria-label="Default select section">
        <option selected>Select Year</option>
        <option value="section1">2021</option>
        <option value="section2">2022</option>
        <option value="section3">2023</option>
    </select>

    <div class="container alternating-table" id="courses">
        <% courses.forEach(course => { %>
            <%- course %>
        <% }) %>
    </div>
    <%- addForm %>
</div>

<script>
    $(document).ready(function() {
        $('.course').click(function(e){
            var course_name = $(this).attr('id')
            window.location.href=`/admin/users?group=${course_name}`;
        })
    })

</script>
